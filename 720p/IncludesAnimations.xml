<?xml version="1.0"?>
<includes>
	<!-- GENERAL -->
	<include name="fadeinout">
		<animation type="WindowOpen">
			<effect type="fade" start="0" end="100" time="350"/>
		</animation>
		<animation type="WindowClose">
			<effect type="fade" start="100" end="0" time="250"/>
		</animation>
		<animation type="Visible">
			<effect type="fade" start="0" end="100" time="350"/>
		</animation>
		<animation type="Hidden">
			<effect type="fade" start="100" end="0" time="250"/>
		</animation>
	</include>
</includes>
